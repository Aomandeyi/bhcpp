#include <iostream>

using namespace std;

struct Object
{
    Object(string const & n)
        : name(n)
    {
        std::cout << "Hello world!" << name << endl;
    }
    
    Object()
        : name("Tom")
    {
        std::cout << "Hello world!" << name << endl;
    }
    
    ~Object()
    {
        std::cout << "Byebye world!" << name << endl;
    }
    
    string name;
};

